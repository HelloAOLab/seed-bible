        "0a0756bd-9e4f-435d-934a-f69e439f0c65/api/ARBNAV/GEN/2.json": {
          "data": {
            "book": {
              "commonName": "التَّكوين",
              "firstChapterApiLink": "/api/ARBNAV/GEN/1.json",
              "id": "GEN",
              "lastChapterApiLink": "/api/ARBNAV/GEN/50.json",
              "name": "تكوين",
              "numberOfChapters": 50,
              "order": 1,
              "sha256": "fed2e7d0fdb9a0a7e4aae7f724ce27612d418f755d3480d1143ddd3194c56db1",
              "title": "كتابُ التَّكوين",
              "totalNumberOfVerses": 1533,
              "translationId": "ARBNAV"
            },
            "chapter": {
              "content": [
                {
                  "content": [
                    "وَهَكَذَا اكْتَمَلَتِ السَّمَاوَاتُ وَالأَرْضُ بِكُلِّ مَا فِيهَا."
                  ],
                  "number": 1,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَفِي الْيَوْمِ السَّابِعِ أَتَمَّ اللهُ عَمَلَهُ الَّذِي قَامَ بِهِ، فَاسْتَرَاحَ فِيهِ مِنْ جَمِيعِ مَا عَمِلَهُ."
                  ],
                  "number": 2,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَبَارَكَ اللهُ الْيَوْمَ السَّابِعَ وَقَدَّسَهُ، لأَنَّهُ اسْتَرَاحَ فِيهِ مِنْ جَمِيعِ أَعْمَالِ الْخَلْقِ"
                  ],
                  "number": 3,
                  "type": "verse"
                },
                {
                  "content": [
                    "هَذَا وَصْفٌ مَبْدَئِيٌّ لِلسَّمَاوَاتِ وَالأَرْضِ يَوْمَ خَلَقَهَا الرَّبُّ الإِلَهُ."
                  ],
                  "number": 4,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَلَمْ يَكُنْ قَدْ نَبَتَ بَعْدُ فِي الأَرْضِ شَجَرٌ بَرِّيٌّ وَلاَ عُشْبٌ بَرِّيٌّ، لأَنَّ الرَّبَّ الإِلَهَ لَمْ يَكُنْ قَدْ أَرْسَلَ مَطَراً عَلَى الأَرْضِ، وَلَمْ يَكُنْ هُنَاكَ إِنْسَانٌ لِيَفْلَحَهَا،"
                  ],
                  "number": 5,
                  "type": "verse"
                },
                {
                  "content": [
                    "إلاَّ أَنَّ ضَبَاباً كَانَ يَتَصَاعَدُ مِنَ الأَرْضِ فَيَسْقِي سَطْحَهَا كُلَّهُ."
                  ],
                  "number": 6,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ جَبَلَ الرَّبُّ الإِلَهُ آدَمَ مِنْ تُرَابِ الأَرْضِ وَنَفَخَ فِي أَنْفِهِ نَسَمَةَ حَيَاةٍ، فَصَارَ آدَمُ نَفْساً حَيَّةً."
                  ],
                  "number": 7,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَأَقَامَ الرَّبُّ الإِلَهُ جَنَّةً فِي شَرْقِيِّ عَدْنٍ وَوَضَعَ فِيهَا آدَمَ الَّذِي جَبَلَهُ."
                  ],
                  "number": 8,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَاستَنْبَتَ الرَّبُّ الإِلَهُ مِنَ الأَرْضِ كُلَّ شَجَرَةٍ بَهِيَّةٍ لِلنَّظَرِ، وَلَذِيذَةٍ لِلأَكْلِ، وَغَرَسَ أَيْضاً شَجَرَةَ الْحَيَاةِ، وَشَجَرَةَ مَعْرِفَةِ الْخَيْرِ وَالشَّرِّ فِي وَسَطِ الْجَنَّةِ."
                  ],
                  "number": 9,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَكَانَ نَهْرٌ يَجْرِي فِي عَدْنٍ لِيَسْقِيَ الْجَنَّةَ، وَمَا يَلْبَثُ أَنْ يَنْقَسِمَ مِنْ هُنَاكَ إِلَى أَرْبَعَةِ أَنْهُرٍ:"
                  ],
                  "number": 10,
                  "type": "verse"
                },
                {
                  "content": [
                    "الأَوَّلُ مِنْهَا يُدْعَى فِيشُونَ، الَّذِي يَلْتَفُّ حَوْلَ كُلِّ الْحَوِيلَةِ حَيْثُ يُوْجَدُ الذَّهَبُ."
                  ],
                  "number": 11,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَذَهَبُ تِلْكَ الأَرْضِ جَيِّدٌ، وَفِيهَا أَيْضاً الْمُقْلُ وَحَجَرُ الْجَزْعِ."
                  ],
                  "number": 12,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَالنَّهْرُ الثَّانِي يُدْعَى جِيحُونَ الَّذِي يُحِيطُ بِجَمِيعِ أَرْضِ كُوشٍ."
                  ],
                  "number": 13,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَالنَّهْرُ الثَّالِثُ يُدْعَى حِدَّاقِلَ وَهُوَ الْجَارِي فِي شَرْقِيِّ أَشُّورَ. وَالنَّهْرُ الرَّابِعُ هُوَ الْفُرَاتُ."
                  ],
                  "number": 14,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَأَخَذَ الرَّبُّ الإِلَهُ آدَمَ وَوَضَعَهُ فِي جَنَّةِ عَدْنٍ لِيَفْلَحَهَا وَيَعْتَنِيَ بِهَا."
                  ],
                  "number": 15,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَأَمَرَ الرَّبُّ الإِلَهُ آدَمَ قَائِلا: «كُلْ مَا تَشَاءُ مِنْ جَمِيعِ أَشْجَارِ الْجَنَّةِ،"
                  ],
                  "number": 16,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَلَكِنْ إِيَّاكَ أَنْ تَأْكُلَ مِنْ شَجَرَةِ مَعْرِفَةِ الْخَيْرِ وَالشَّرِّ لأَنَّكَ حِينَ تَأْكُلُ مِنْهَا حَتْماً تَمُوت»."
                  ],
                  "number": 17,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ قَالَ الرَّبُّ الإِلَهُ: «لَيْسَ جَيِّداً أَنْ يَبْقَى آدَمُ وَحِيداً. سَأَصْنَعُ لَهُ مُعِيناً مُشَابِهاً لَهُ»."
                  ],
                  "number": 18,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَكَانَ الرَّبُّ الإِلَهُ قَدْ جَبَلَ مِنَ التُّرَابِ كُلَّ وُحُوشِ الْبَرِّيَّةِ وَطُيُورِ الْفَضَاءِ وَأَحْضَرَهَا إِلَى آدَمَ لِيَرَى بِأَيِّ أَسْمَاءٍ يَدْعُوهَا، فَصَارَ كُلُّ اسْمٍ أَطْلَقَهُ آدَمُ عَلَى كُلِّ مَخْلُوقٍ حَيٍّ اسْماً لَهُ."
                  ],
                  "number": 19,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَهَكَذَا أَطْلَقَ آدَمُ أَسْمَاءً عَلَى كُلِّ الطُّيُورِ وَالْحَيَوَانَاتِ وَالْبَهَائِمِ. غَيْرَ أَنَّهُ لَمْ يَجِدْ لِنَفْسِهِ مُعِيناً مُشَابِهاً لَهُ."
                  ],
                  "number": 20,
                  "type": "verse"
                },
                {
                  "content": [
                    "فَأَوْقَعَ الرَّبُّ الإِلَهُ آدَمَ فِي نَوْمٍ عَمِيقٍ، ثُمَّ تَنَاوَلَ ضِلْعاً مِنْ أَضْلاَعِهِ وَسَدَّ مَكَانَهَا بِاللَّحْمِ،"
                  ],
                  "number": 21,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَعَمِلَ مِنْ هَذِهِ الضِّلْعِ امْرَأَةً أَحْضَرَهَا إِلَى آدَمَ."
                  ],
                  "number": 22,
                  "type": "verse"
                },
                {
                  "content": [
                    "فَقَالَ آدَمُ: «هَذِهِ الآنَ عَظْمٌ مِنْ عِظَامِي وَلَحْمٌ مِنْ لَحْمِي. فَهِيَ تُدْعَى امْرَأَةً لأَنَّهَا مِنِ امْرِئٍ أُخِذَتْ»."
                  ],
                  "number": 23,
                  "type": "verse"
                },
                {
                  "content": [
                    "لِهَذَا، فَإِنَّ الرَّجُلَ يَتْرُكُ أَبَاهُ وَأُمَّهُ وَيَلْتَصِقُ بِامْرَأَتِهِ، وَيَصِيرَانِ جَسَداً وَاحِداً."
                  ],
                  "number": 24,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَكَانَ آدَمُ وَامْرَأَتُهُ عُرْيَانَيْنِ، وَلَمْ يَخْجَلَا مِنْ ذَلِكَ."
                  ],
                  "number": 25,
                  "type": "verse"
                }
              ],
