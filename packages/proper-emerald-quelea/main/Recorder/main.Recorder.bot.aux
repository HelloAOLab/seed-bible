        "0a0756bd-9e4f-435d-934a-f69e439f0c65GEN 1": {
          "data": {
            "book": {
              "commonName": "التَّكوين",
              "firstChapterApiLink": "/api/ARBNAV/GEN/1.json",
              "id": "GEN",
              "lastChapterApiLink": "/api/ARBNAV/GEN/50.json",
              "name": "تكوين",
              "numberOfChapters": 50,
              "order": 1,
              "sha256": "fed2e7d0fdb9a0a7e4aae7f724ce27612d418f755d3480d1143ddd3194c56db1",
              "title": "كتابُ التَّكوين",
              "totalNumberOfVerses": 1533,
              "translationId": "ARBNAV"
            },
            "chapter": {
              "content": [
                {
                  "content": [
                    "فِي الْبَدْءِ خَلَقَ اللهُ السَّمَاوَاتِ وَالأَرْضَ،"
                  ],
                  "number": 1,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَإِذْ كَانَتِ الأَرْضُ مُشَوَّشَةً وَمُقْفِرَةً وَتَكْتَنِفُ الظُّلْمَةُ وَجْهَ الْمِيَاهِ، وَإِذْ كَانَ رُوحُ اللهِ يُرَفْرِفُ عَلَى سَطْحِ الْمِيَاهِ،"
                  ],
                  "number": 2,
                  "type": "verse"
                },
                {
                  "content": [
                    "أَمَرَ اللهُ: «لِيَكُنْ نُورٌ». فَصَارَ نُورٌ،"
                  ],
                  "number": 3,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَرَأَى اللهُ النُّورَ فَاسْتَحْسَنَهُ وَفَصَلَ بَيْنَهُ وَبَيْنَ الظَّلامِ."
                  ],
                  "number": 4,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَسَمَّى اللهُ النُّورَ نَهَاراً، أَمَّا الظَّلامُ فَسَمَّاهُ لَيْلاً. وَهَكَذَا جَاءَ مَسَاءٌ أَعْقَبَهُ صَبَاحٌ، فَكَانَ الْيَوْمَ الأَوَّلَ."
                  ],
                  "number": 5,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ أَمَرَ اللهُ: «لِيَكُنْ جَلَدٌ يَحْجُزُ بَيْنَ مِيَاهٍ وَمِيَاهٍ»."
                  ],
                  "number": 6,
                  "type": "verse"
                },
                {
                  "content": [
                    "فَخَلَقَ اللهُ الْجَلَدَ، وَفَرَّقَ بَيْنَ الْمِيَاهِ الَّتِي تَحْمِلُهَا السُّحُبُ وَالْمِيَاهِ الَّتِي تَغْمُرُ الأَرْضَ. وَهَكَذَا كَانَ."
                  ],
                  "number": 7,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَسَمَّى اللهُ الْجَلَدَ سَمَاءً. ثُمَّ جَاءَ مَسَاءٌ أَعْقَبَهُ صَبَاحٌ فَكَانَ الْيَوْمَ الثَّانِي."
                  ],
                  "number": 8,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ أَمَرَ اللهُ: «لِتَتَجَمَّعِ الْمِيَاهُ الَّتِي تَحْتَ السَّمَاءِ إِلَى مَوْضِعٍ وَاحِدٍ، وَلْتَظْهَرِ الْيَابِسَةُ». وَهَكَذَا كَانَ."
                  ],
                  "number": 9,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَسَمَّى اللهُ الْيَابِسَةَ أَرْضاً وَالْمِيَاهَ الْمُجْتَمِعَةَ بِحَاراً. وَرَأَى اللهُ ذَلِكَ فَاسْتَحْسَنَهُ."
                  ],
                  "number": 10,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَأَمَرَ اللهُ: «لِتُنْبِتِ الأَرْضُ خُضْرَةً، وَشَجَراً مُثْمِراً فِيهِ بِزْرُهُ الَّذِي يُنْتِجُ ثَمَراً كَجِنْسِهِ فِي الأَرْضِ». وَهَكَذَا كَانَ."
                  ],
                  "number": 11,
                  "type": "verse"
                },
                {
                  "content": [
                    "فَأَنْبَتَتِ الأَرْضُ كُلَّ أَنْوَاعِ الأَعْشَابِ وَالْبُقُولِ الَّتِي تَحْمِلُ بُزُوراً مِنْ جِنْسِهَا، وَالأَشْجَارَ الَّتِي تَحْمِلُ أَثْمَاراً ذَاتَ بُذُورٍ حَسَبَ نَوْعِهَا. وَرَأَى اللهُ ذَلِكَ فَاسْتَحْسَنَهُ."
                  ],
                  "number": 12,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَجَاءَ مَسَاءٌ أَعْقَبَهُ صَبَاحٌ فَكَانَ الْيَوْمَ الثَّالِثَ."
                  ],
                  "number": 13,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ أَمَرَ اللهُ: «لِتَكُنْ أَنْوَارٌ فِي قُبَّةِ السَّمَاءِ لِتُفَرِّقَ بَيْنَ النَّهَارِ وَاللَّيْلِ، فَتَكُونَ عَلاَمَاتٍ لِتَحْدِيدِ أَزْمِنَةٍ وَأَيَّامٍ وَسِنِينَ."
                  ],
                  "number": 14,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَتَكُونَ أَيْضاً أَنْوَاراً فِي قُبَّةِ السَّمَاءِ لِتُضِيءَ الأَرْضَ». وَهَكَذَا كَانَ."
                  ],
                  "number": 15,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَخَلَقَ اللهُ نُورَ يْنِ عَظِيمَيْنِ، النُّورَ الأَكْبَرَ لِيُشْرِقَ فِي النَّهَارِ، وَالنُّورَ الأَصْغَرَ لِيُضِيءَ فِي اللَّيْلِ، كَمَا خَلَقَ النُّجُومَ أَيْضاً."
                  ],
                  "number": 16,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَجَعَلَهَا اللهُ فِي قُبَّةِ السَّمَاءِ لِتُضِيءَ الأَرْضَ،"
                  ],
                  "number": 17,
                  "type": "verse"
                },
                {
                  "content": [
                    "لِتَتَحَكَّمَ بِالنَّهَارِ وَبِاللَّيْلِ وَلِتُفَرِّقَ بَيْنَ النُّورِ وَالظَّلامِ. وَرَأَى اللهُ ذَلِكَ فَاسْتَحْسَنَهُ."
                  ],
                  "number": 18,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَجَاءَ مَسَاءٌ أَعْقَبَهُ صَبَاحٌ فَكَانَ الْيَوْمَ الرَّابِعَ."
                  ],
                  "number": 19,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ أَمَرَ اللهُ: «لِتَمْتَلِئِ الْمِيَاهُ بِشَتَّى الْحَيَوَانَاتِ الْحَيَّةِ وَلْتُحَلِّقِ الطُّيُورُ فَوْقَ الأَرْضِ عَبْرَ فَضَاءِ السَّمَاءِ»."
                  ],
                  "number": 20,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَهَكَذَا خَلَقَ اللهُ الْحَيَوَانَاتِ الْمَائِيَّةَ الضَّخْمَةَ، وَالْكَائِنَاتِ الْحَيَّةَ الَّتِي امْتَلأَتْ بِهَا الْمِيَاهُ، كُلًّا حَسَبَ أَجْنَاسِهَا، وَأَيْضاً الطُّيُورَ وَفْقاً لأَنْوَاعِهَا. وَرَأَى اللهُ ذَلِكَ فَاسْتَحْسَنَهُ."
                  ],
                  "number": 21,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَبَارَكَهَا اللهُ قَائِلاً: «انْتِجِي، وَتَكَاثَرِي وَامْلإِي مِيَاهَ الْبِحَارِ. وَلْتَتَكَاثَرِ الطُّيُورُ فَوْقَ الأَرْضِ»."
                  ],
                  "number": 22,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ جَاءَ مَسَاءٌ أَعْقَبَهُ صَبَاحٌ فَكَانَ الْيَوْمَ الْخَامِسَ."
                  ],
                  "number": 23,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ أَمَرَ اللهُ: «لِتُخْرِجِ الأَرْضُ كَائِنَاتٍ حَيَّةً، كُلاً حَسَبَ جِنْسِهَا، مِنْ بَهَائِمَ وَزَوَاحِفَ وَوُحُوشٍ وَفْقاً لأَنْوَاعِهَا». وَهَكَذَا كَانَ."
                  ],
                  "number": 24,
                  "type": "verse"
                },
                {
                  "content": [
                    "فَخَلَقَ اللهُ وُحُوشَ الأَرْضِ، وَالْبَهَائِمَ وَالزَّوَاحِفَ، كُلاً حَسَبَ نَوْعِهَا. وَرَأَى اللهُ ذَلِكَ فَاسْتَحْسَنَهُ."
                  ],
                  "number": 25,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ قَالَ اللهُ: «لِنَصْنَعِ الإِنْسَانَ عَلَى صُورَتِنَا، كَمِثَالِنَا، فَيَتَسَلَّطَ عَلَى سَمَكِ الْبَحْرِ، وَعَلَى طَيْرِ السَّمَاءِ، وَعَلَى الأَرْضِ، وَعَلَى كُلِّ زَاحِفٍ يَزْحَفُ عَلَيْهَا»."
                  ],
                  "number": 26,
                  "type": "verse"
                },
                {
                  "content": [
                    "فَخَلَقَ اللهُ الإِنْسَانَ عَلَى صُورَتِهِ. عَلَى صُورَةِ اللهِ خَلَقَهُ. ذَكَراً وَأُنْثَى خَلَقَهُمْ."
                  ],
                  "number": 27,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَبَارَكَهُمُ اللهُ قَائِلاً لَهُمْ: «أَثْمِرُوا وَتَكَاثَرُوا وَامْلَأُوا الأَرْضَ وَأَخْضِعُوهَا. وَتَسَلَّطُوا عَلَى سَمَكِ الْبَحْرِ، وَعَلَى طَيْرِ السَّمَاءِ وَعَلَى كُلِّ حَيَوَانٍ يَتَحَرَّكُ عَلَى الأَرْضِ»."
                  ],
                  "number": 28,
                  "type": "verse"
                },
                {
                  "content": [
                    "ثُمَّ قَالَ لَهُمْ: «إِنِّي قَدْ أَعْطَيْتُكُمْ كُلَّ أَصْنَافِ النَّبَاتَاتِ ذَاتِ الْبُذُورِ الْمُنْتَشِرَةِ عَلَى كُلِّ سَطْحِ الأَرْضِ، وَكُلَّ شَجَرٍ يَحْمِلُ ثَمَراً فِيهِ بُذُورٌ، لِتَكُونَ لَكُمْ طَعَاماً."
                  ],
                  "number": 29,
                  "type": "verse"
                },
                {
                  "content": [
                    "أَمَّا الْعُشْبُ الأَخْضَرُ فَقَدْ جَعَلْتُهُ طَعَاماً لِوُحُوشِ الأَرْضِ وَلِطُيُورِ السَّمَاءِ وَالْحَيَوَانَاتِ الزَّاحِفَةِ، وَلجَمِيعِ الْكَائِنَاتِ الْحَيَّةِ». وَهَكَذَا كَانَ."
                  ],
                  "number": 30,
                  "type": "verse"
                },
                {
                  "content": [
                    "وَرَأَى اللهُ مَا خَلَقَهُ فَاسْتَحْسَنَهُ جِدّاً. ثُمَّ جَاءَ مَسَاءٌ أَعْقَبَهُ صَبَاحٌ فَكَانَ الْيَوْمَ السَّادِسَ."
                  ],
                  "number": 31,
                  "type": "verse"
                }
              ],
              "footnotes": [
              ],
              "number": 1
